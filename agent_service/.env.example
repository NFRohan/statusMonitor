# StatusMonitor Agent Configuration
# Copy this file to .env and fill in your agent token

# Get your token from the StatusMonitor web interface:
# 1. Login to http://localhost:5173
# 2. Go to "My Agents" 
# 3. Click "New Agent" and copy the token

AGENT_TOKEN=your_agent_token_here

# Ingestion service URL (default for local development)
INGESTION_URL=http://localhost:8001/ingest

# Metrics collection interval in seconds
INTERVAL=5
